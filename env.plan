(define quote (vau (x) _ x))
(define if (vau (condition if-true if-false) env
                (eval env
                      (bool (eval env condition) if-true if-false))))
(define cons (vau (x xs) env
                  ((eval env x) . (eval env xs))))
(define list (vau xs env
                  (if (null? xs)
                    ()
                    ((eval env (car xs)) . (eval env (xs . (cdr xs)))))))
